
public class Commission {
	public static double Calculate(int lock, int stock, int barrel) {
		double sale = (45 * lock) + (30 * stock) + (25 * barrel);
		if (sale < 1000)
			return (sale * 0.1);
		else if (sale >= 1000)
			return (0.15 * (sale - 1000)) + (1000 * 0.10);
		else if (sale > 1800)
			return (0.2 * (sale - 1800)) + (800 * 0.15) + (1000 * 0.1);
		return 0.0;
	}

}
